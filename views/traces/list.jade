extends ../base.jade

block content
  h3 Traces

  each trace in traces
    .media
      a(href=trace.imageUrl)
        .media-body
          div= trace.imageUrl

  if !traces.length
    p No traces found.

  if nextPageToken
    nav
      ul.pager
        li
          a(href="?pageToken=#{encodeURIComponent(nextPageToken)}") More
